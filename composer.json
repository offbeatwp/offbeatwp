{
    "name": "offbeatwp/offbeatwp",
    "description": "The OffbeatWP base theme",
    "license": "MIT",
    "type": "wordpress-theme",
    "config": {
        "secure-http": false
    },
    "require": {
        "offbeatwp/framework": "dev-master",
        "offbeatwp/twig": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app",
            "Modules\\": "modules",
            "Components\\": "components"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
